{
  "schema": {
    "Screens": [
      {
        "navTitle": "Demographic Data",
        "name": "DemographicData"
      },
      {
        "navTitle": "Employment Information",
        "name": "EmploymentInformation"
      },
      {
        "navTitle": "Employer Form",
        "name": "EmployerForm"
      },
      {
        "navTitle": "Employees Grid",
        "name": "EmployeesGrid"
      }
    ],
    "Input": {
      "type": "string"
    },
    "Gender": {
      "type": "string",
      "title": "Gender",
      "enum": [ "Male", "Female", "Other", "Trans" ]
    },
    "YearofGraduation": {
      "type": "string",
      "title": "Year of Graduation",
      "enum": [ "2000", "2001", "2002", "2003", "2004", "2005", "2006", "2007", "2008", "2009", "2010", "2011", "2012", "2013", "2014", "2015", "2016", "2017", "2018", "2019", "2020" ]
    },
    "IsPartnerLRAPParticipant": {
      "type": "string",
      "title": "title",
      "enum": [ "Yes", "No" ]
    },
    "Country": {
      "type": "string",
      "title": "Country",
      "enum": [ "Bangladesh", "India", "United States" ]
    },
    "Address1": {
      "type": "string",
      "enum": [ "New York", "Florida", "Texas" ]
    },
    "ScholarshipProgram": {
      "type": "string",
      "enum": [ "AnBryce Scholarship", "Furman Academic Scholarship", "Furman Public Policy Scholarship", "Institute for International Law and Justice Scholarship", "Latinx Rights Scholarship", "Leadership Program in Law and Business Scholarship", "Root-Tilden-Kern Public Interest Scholarship", "Sinsheimer Service Scholarship", "None of the above" ]
    },
    "EmployementOption": {
      "title": "Please choose one of the following that best describes your employment prior to this application",
      "type": "string",
      "enum": [ "I am a first time applicant. I have been employed in an LRAP eligible position since graduating from NYU School of Law.", "I am a first time applicant. I have been employed in non-LRAP eligible employment (typically a private firm) since graduating from NYU School of Law.", "I am continuing/returning participant. I have been employed in an LRAP eligible position since graduating from NYU School of Law.", "I am continuing/returning participant. I have previously submitted the Non-LRAP Eligible Employment Form and have remained in LRAP eligible employment since my previous submission.", "I am continuing/returning participant. I am renewing my application to earn forgiveness and do not anticipate being eligible for LRAP benefits." ]
    },
    "EmployedByMoreThanOneEmployer": {
      "title": "Will you be employed by more than one employer at the same time during teh program year?",
      "type": "string",
      "enum": [ "Yes", "No" ]
    },
    "AnticipateInRepaymentOfEducationalLoans": {
      "title": "Do you anticipate receiving any assistance in the repayment of educational loans from an organization other than NYU? (Please note: if yes, you will be asked to provide additional documentation describing these benefits)",
      "type": "string",
      "enum": [ "Yes", "No" ]
    },
    "TypeOfAssistance": {
      "title": "Type of assistance",
      "type": "string",
      "enum": [ "Employer", "State", "Federal", "Non-Profit", "Other" ]
    },
    "AnticipateAdditionalIncome": {
      "title": "Do you anticipate any additional income from employment not listed on this application?",
      "type": "string",
      "enum": [ "Yes", "No" ]
    },
    "EmployerType": {
      "title": "Employer Type",
      "type": "string",
      "enum": [ "Type 1", "Type 2", "Type 3" ]
    },
    "AreYouPaid": {
      "title": "How Often Are You Paid",
      "type": "string",
      "enum": [ "Monthly", "Half-Yearly", "Yearly" ]
    }
  },
