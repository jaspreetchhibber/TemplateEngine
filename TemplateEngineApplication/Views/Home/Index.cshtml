    <svg id="SvgjsSvg1001" width="2" height="0" xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svgjs="http://svgjs.com/svgjs" style="overflow: hidden; top: -100%; left: -100%; position: absolute; opacity: 0;"><defs id="SvgjsDefs1002"></defs><polyline id="SvgjsPolyline1003" points="0,0"></polyline><path id="SvgjsPath1004" d="M0 0 "></path></svg>
    <script type="text/javascript">
        $(document).ready(function () {
            GetJsonDataAndAddForm();
        });

        function GetJsonDataAndAddForm() {
            fetch(`Home/GetJsonFile`)
                .then(response => response.json())
                .then(form => {
                    if (form != undefined && form != "") {
                        const jsonform = JSON.parse(form.file);
                        let form_demographticdata = JSON.parse(form.file);
                        let form_EmployementInformation = JSON.parse(form.file);
                        let form_EmployerForm = JSON.parse(form.file);
                        let form_EmployerGrid = JSON.parse(form.file);

                        var demographticdata = RemoveOtherForm(form_demographticdata.form, "DemographicData");
                        var employementInformation = RemoveOtherForm(form_EmployementInformation.form, "EmployementInformation");
                        var employerForm = RemoveOtherForm(form_EmployerForm.form, "EmployerForm");
                        var employerGrid = RemoveOtherForm(form_EmployerGrid.form, "EmployerGrid");

                        form_demographticdata.form = demographticdata;``
                        form_EmployementInformation.form = employementInformation;
                        form_EmployerForm.form = employerForm;
                        form_EmployerGrid.form = employerGrid;

                        $('.form_DemographicData').jsonForm(form_demographticdata);
                        $('.form_EmployementInformation').jsonForm(form_EmployementInformation);
                        $('.form_EmployerForm').jsonForm(form_EmployerForm);
                        $('.form_EmployerGrid').jsonForm(form_EmployerGrid);

                        var screenArray = jsonform.schema.Screens;
                        var sidebar = $("#ul_sidebar");
                        var html = "";
                        $(screenArray).each(function (index) {
                            html += "<li class='menu-item'>";
                            html += "<a id='" + $(this)[0].name + "' class='menu-link'>";
                            html += "<i class='menu-icon fas fa-th-large'></i>";
                            html += "<span class='menu-text'>" + $(this)[0].navTitle + "</span>";
                            html += "</a>";
                            html += "</li>";

                        });

                        sidebar.html(html);
                    }
                    ShowForms();
                })
                .catch(error => console.error('Unable to get item.', error));
        }
        function ShowForms() {
            $("#divDemographicData").show();
            $("#divEmploymentInformation").hide();
            $("#divEmployerForm").hide();
            $("#divEmployerGrid").hide();

            $("#EmploymentInformation").click(function () {
                $("#divDemographicData").hide();
                $("#divEmploymentInformation").show();
                $("#divEmployerForm").hide();
                $("#divEmployerGrid").hide();
            });
            $("#DemographicData").click(function () {
                $("#divDemographicData").show();
                $("#divEmploymentInformation").hide();
                $("#divEmployerForm").hide();
                $("#divEmployerGrid").hide();
            });
            $("#EmployerForm").click(function () {
                $("#divEmployerForm").show();
                $("#divDemographicData").hide();
                $("#divEmploymentInformation").hide();
                $("#divEmployerGrid").hide();
            });
            $("#EmployeesGrid").click(function () {
                $("#divEmployerGrid").show();
                $("#divDemographicData").hide();
                $("#divEmploymentInformation").hide();
                $("#divEmployerForm").hide();
            });
        }
        function RemoveOtherForm(mainform, value) {
            return mainform.filter(ele => ele.id == value);
        }

    </script>

